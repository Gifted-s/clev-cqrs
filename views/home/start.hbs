            </div>

        </div>
    </div>
    {{# each this.events}}
    <div class="container cc">
        <div class="row nn ">
            <div class="col-md-2">
                {{this.eventId}}

            </div>
            <div class="col-md-2">
                {{this.eventName}}

            </div>
            <div class="col-md-2">
                {{this.eventTime}}
            </div>
            <div class="col-md-6">
                {{this.data}}
            </div>

        </div>
    </div>
    {{/ each}}

</div>
{{/ each}}
</div>
  </div>
<div class="mb-5">

</div>