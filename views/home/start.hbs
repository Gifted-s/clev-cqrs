<div class="container-fluid pp">
    <h2 class="text-center ttt">{{name}}s</h2>


   <div class="ff">

 
    {{# each event}}

    <div class="container yy">
        <div style="background-color: white;" class="it row">


            <div class="col-md-12">
                <h6 class="text-center mb-4 mt-3">Aggregate id : {{this.id}}</h6>
            </div>
            <div class="col-md-2">
                <h5> Event id</h5>
            </div>
            <div class="col-md-2">
